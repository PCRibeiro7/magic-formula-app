      "/api/v2/inflation": {
        "get": {
          "tags": ["inflation"],
          "summary": "Get the history of the inflation of a certain country",
          "parameters": [
            {
              "name": "country",
              "in": "query",
              "description": "Get data for a certain country, defaults to 'brazil'",
              "required": false,
              "style": "form",
              "schema": {
                "type": "string"
              },
              "example": "brazil"
            },
            {
              "name": "historical",
              "in": "query",
              "description": "Get historical data for a certain country, defaults to 'false'",
              "required": false,
              "style": "form",
              "schema": {
                "type": "boolean"
              },
              "example": "false"
            },
            {
              "name": "start",
              "in": "query",
              "description": "Get historical data from a certain date, defaults to today's date (DD/MM/YYYY)",
              "required": false,
              "style": "form",
              "schema": {
                "type": "string"
              },
              "example": "29/10/2022"
            },
            {
              "name": "end",
              "in": "query",
              "description": "Get historical data until a certain date, defaults to today's date (DD/MM/YYYY)",
              "required": false,
              "style": "form",
              "schema": {
                "type": "string"
              },
              "example": "29/10/2022"
            },
            {
              "name": "sortBy",
              "in": "query",
              "description": "Sort the data by a column, defaults to 'date'",
              "required": false,
              "style": "form",
              "schema": {
                "type": "string",
                "enum": ["date", "value"]
              },
              "example": "date"
            },
            {
              "name": "sortOrder",
              "in": "query",
              "description": "Sort order for sortBy, defaults to 'desc'",
              "required": false,
              "style": "form",
              "schema": {
                "type": "string",
                "enum": ["desc", "asc"]
              },
              "example": "desc"
            }
          ],
          "responses": {
            "200": {
              "description": "Successful response",
              "content": {
                "application/json": {
                  "example": {
                    "inflation": [
                      {
                        "date": "01/09/2022",
                        "value": "7.17",
                        "epochDate": 1661990400000
                      }
                    ]
                  }
                }
              }
            }
          }
        }
      },
      "/api/v2/inflation/available": {
        "get": {
          "tags": ["inflation"],
          "summary": "Get all available supported inflation countries",
          "parameters": [
            {
              "name": "search",
              "in": "query",
              "description": "Find all possible supported inflation countries, if no query is passed then all supported inflation countries are returned",
              "required": false,
              "style": "form",
              "schema": {
                "type": "string"
              },
              "example": "br"
            }
          ],
          "responses": {
            "200": {
              "description": "Successful response",
              "content": {
                "application/json": {
                  "example": {
                    "countries": ["brazil"]
                  }
                }
              }
            },
            "404": {
              "description": "Country not found",
              "content": {
                "application/json": {
                  "example": {
                    "message": "Country not found"
                  }
                }
              }
            }
          }
        }
      },
